<Window x:Class="SQLEasyManager.KopiowanieUprawnienUzytkownikow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Kopiowanie Uprawnień Użytkowników" Height="200" Width="400">
    <Grid>
        <Label Content="Źródłowy użytkownik:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
        <ComboBox x:Name="SourceUserComboBox" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Margin="150,10,0,0"/>

        <Label Content="Docelowy użytkownik:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,50,0,0"/>
        <ComboBox x:Name="TargetUserComboBox" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Margin="150,50,0,0"/>

        <Button Content="Kopiuj Uprawnienia" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Margin="100,100,0,0" Click="KopiujUprawnienia_Click"/>
    </Grid>
</Window>
